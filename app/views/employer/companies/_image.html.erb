<%= f.fields_for :images do |builder| %>
  <div class="form-group form-inline">
    <label class="control-label col-md-3 col-sm-3 col-xs-12"><%= t ".image" %></label>
    <%= builder.file_field :picture, class: "upload-image" %>
    <div id="preview" class="col-md-3 col-md-offset-3"></div>
  </div>
<% end %>
